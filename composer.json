{
    "name": "inisiatif/model-shared",
    "description": "Share model at Inisiatif Zakat Indonesia",
    "license": "proprietary",
    "type": "library",
    "require": {
        "php": "~8.0.0|~8.1.0",
        "spatie/laravel-query-builder": "^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Inisiatif\\ModelShared\\": "src/",
            "Inisiatif\\ModelShared\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inisiatif\\ModelShared\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inisiatif\\ModelShared\\Providers\\ModelSharedServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
