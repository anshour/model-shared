{
    "name": "inisiatif/model-shared",
    "description": "Share model package using in Inisiatif Zakat Indonesia",
    "license": "proprietary",
    "type": "library",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/inisiatif/"
        },
        {
            "packagist.org": false
        }
    ],
    "require": {
        "php": "~8.0",
        "illuminate/database": "^8.26",
        "illuminate/routing": "^8.26",
        "illuminate/support": "^8.26",
        "inisiatif/common": "^1.1",
        "inisiatif/contract": "^1.1",
        "spatie/laravel-query-builder": "^3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^5.10|^6.7"
    },
    "autoload": {
        "psr-4": {
            "Inisiatif\\ModelShared\\": "src/",
            "Inisiatif\\ModelShared\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inisiatif\\ModelShared\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
